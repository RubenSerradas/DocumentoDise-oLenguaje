/**
 * Implementacion de algoritmo de euclides en Guava.
 */
function void main(){
  declare {
    integer var a;
    integer var b;
    integer var t;
  }
  print("a: ");
  read( a ); print('\n');
  print("b: ");
  read( b ); print('\n'); 

  while (b != 0) do{
      t := b;
      b := a mod b;
      a := t;
  }
  print(a);
}
